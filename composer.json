{
    "name": "louishrg/state-flow",
    "description": "Simple implementation of state machine for Laravel",
    "keywords": [
        "louishrg",
        "state-flow",
        "state-machine"
    ],
    "homepage": "https://github.com/louishrg/state-flow",
    "license": "MIT",
    "version": "1.2.1",
    "type": "library",
    "authors": [
        {
            "name": "Louis Harang",
            "email": "dev@narah.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^7.2|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit" : "^8.0|^9.0",
        "orchestra/testbench": "~3.8.0|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Louishrg\\StateFlow\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Louishrg\\StateFlow\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Louishrg\\StateFlow\\StateFlowServiceProvider"
            ]
        }
    }
}
